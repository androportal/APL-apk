package your.scilab.icon;
 
import android.app.Activity;
import android.os.Bundle;
import android.webkit.WebView;
 
public class scilabview extends Activity {
 
	private WebView scilabview;
 
	public void onCreate(Bundle savedInstanceState) {
		super.onCreate(savedInstanceState);
		setContentView(R.layout.scilabview);
 
		scilabview = (WebView) findViewById(R.id.scilabview);
		scilabview.getSettings().setJavaScriptEnabled(true);
		scilabview.loadUrl("http://10.2.2.0/html/scilab");
 
	}
 
}